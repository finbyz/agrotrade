 "doctype": "Print Format",
 "font": "Default",
 "format_data": "[{\"fieldname\": \"print_heading_template\", \"fieldtype\": \"Custom HTML\", \"options\": \"<div class=\\\"print-heading\\\">\\n\\t<h2>\\n\\t\\tTAX INVOICE<br>\\n\\t\\t<small>{{ doc.name }}</small>\\n\\t</h2>\\n</div>\\n<h2 class=\\\"text-center\\\">\\n\\t{% if doc.invoice_copy -%}\\n\\t\\t<small>{{ doc.invoice_copy }}</small>\\n\\t{% endif -%}\\n</h2>\"}, {\"fieldtype\": \"Section Break\", \"label\": \"\"}, {\"fieldtype\": \"Column Break\"}, {\"print_hide\": 0, \"fieldname\": \"company\", \"label\": \"Company\"}, {\"print_hide\": 0, \"fieldname\": \"company_address_display\", \"label\": \"Company Address\"}, {\"fieldtype\": \"Column Break\"}, {\"print_hide\": 0, \"fieldname\": \"posting_date\", \"label\": \"Date\"}, {\"print_hide\": 0, \"fieldname\": \"due_date\", \"label\": \"Payment Due Date\"}, {\"print_hide\": 0, \"fieldname\": \"reverse_charge\", \"label\": \"Reverse Charge\"}, {\"fieldtype\": \"Section Break\", \"label\": \"\"}, {\"fieldtype\": \"Column Break\"}, {\"print_hide\": 0, \"label\": \"Custom HTML\", \"fieldname\": \"_custom_html\", \"options\": \"<hr>\", \"fieldtype\": \"HTML\"}, {\"fieldtype\": \"Section Break\", \"label\": \"Address\"}, {\"fieldtype\": \"Column Break\"}, {\"print_hide\": 0, \"fieldname\": \"customer_name\", \"label\": \"Customer Name\"}, {\"print_hide\": 0, \"fieldname\": \"address_display\", \"label\": \"Address\"}, {\"print_hide\": 0, \"fieldname\": \"contact_display\", \"label\": \"Contact\"}, {\"fieldtype\": \"Column Break\"}, {\"print_hide\": 0, \"fieldname\": \"shipping_address\", \"label\": \"Shipping Address\"}, {\"fieldtype\": \"Section Break\", \"label\": \"\"}, {\"fieldtype\": \"Column Break\"}, {\"visible_columns\": [{\"print_hide\": 0, \"fieldname\": \"item_code\", \"print_width\": \"\"}, {\"print_hide\": 0, \"fieldname\": \"description\", \"print_width\": \"200px\"}, {\"print_hide\": 0, \"fieldname\": \"gst_hsn_code\", \"print_width\": \"\"}, {\"print_hide\": 0, \"fieldname\": \"serial_no\", \"print_width\": \"\"}, {\"print_hide\": 0, \"fieldname\": \"qty\", \"print_width\": \"\"}, {\"print_hide\": 0, \"fieldname\": \"uom\", \"print_width\": \"\"}, {\"print_hide\": 0, \"fieldname\": \"rate\", \"print_width\": \"\"}, {\"print_hide\": 0, \"fieldname\": \"amount\", \"print_width\": \"\"}], \"print_hide\": 0, \"fieldname\": \"items\", \"label\": \"Items\"}, {\"fieldtype\": \"Section Break\", \"label\": \"\"}, {\"fieldtype\": \"Column Break\"}, {\"fieldtype\": \"Column Break\"}, {\"print_hide\": 0, \"fieldname\": \"total\", \"label\": \"Total\"}, {\"fieldtype\": \"Section Break\", \"label\": \"\"}, {\"fieldtype\": \"Column Break\"}, {\"visible_columns\": [{\"print_hide\": 0, \"fieldname\": \"description\", \"print_width\": \"300px\"}], \"print_hide\": 0, \"fieldname\": \"taxes\", \"label\": \"Sales Taxes and Charges\"}, {\"fieldtype\": \"Section Break\", \"label\": \"\"}, {\"fieldtype\": \"Column Break\"}, {\"fieldtype\": \"Column Break\"}, {\"print_hide\": 0, \"fieldname\": \"grand_total\", \"label\": \"Grand Total\"}, {\"print_hide\": 0, \"fieldname\": \"rounded_total\", \"label\": \"Rounded Total\"}, {\"print_hide\": 0, \"fieldname\": \"in_words\", \"label\": \"In Words\"}, {\"fieldtype\": \"Section Break\", \"label\": \"\"}, {\"fieldtype\": \"Column Break\"}, {\"print_hide\": 0, \"fieldname\": \"other_charges_calculation\", \"align\": \"left\", \"label\": \"Tax Breakup\"}, {\"fieldtype\": \"Section Break\", \"label\": \"Terms\"}, {\"fieldtype\": \"Column Break\"}, {\"print_hide\": 0, \"fieldname\": \"terms\", \"label\": \"Terms and Conditions Details\"}]",
 "html": "<!-- gst tax invoice-->\r\n<style>\r\n    body:last-child .print-format td img{\r\n        width: auto !important;\r\n    }\r\n    img.signature{\r\n        height: 120px !important;\r\n        width: auto !important;\r\n    }\r\n    .tbspace>tbody>tr>td {\r\n        padding: 0 2px 0 2px !important;\r\n        margin: 0 !important;\r\n        border-spacing: 0 !important;\r\n    }\r\n\r\n    .tbspace-sub>tbody>tr>td {\r\n        padding: 0 2px 0 2px !important;\r\n        margin: 0 !important;\r\n        border-spacing: 0 !important;\r\n    }\r\n\r\n    .ontop {\r\n        border-top: 1px solid !important;\r\n    }\r\n\r\n    .onright {\r\n        border-right: 1px solid !important;\r\n    }\r\n\r\n    .onleft {\r\n        border-left: 1px solid !important;\r\n    }\r\n\r\n    .onbottom {\r\n        border-bottom: 1px solid !important;\r\n    }\r\n\r\n    .noright {\r\n        border-right: 0 !important;\r\n    }\r\n\r\n    .noleft {\r\n        border-left: 0 !important;\r\n    }\r\n\r\n    .noleftright {\r\n        border-right: 0 !important;\r\n        border-left: 0 !important;\r\n    }\r\n\r\n    .double {\r\n        border-bottom-style: double;\r\n        padding-bottom: 3px !important;\r\n    }\r\n\r\n    .notop {\r\n        border-top: 0 !important;\r\n    }\r\n\r\n    .nobottom {\r\n        border-bottom: 0 !important;\r\n    }\r\n\r\n    .notopbottom {\r\n        border-top: 0 !important;\r\n        border-bottom: 0 !important;\r\n        padding-top: 0 !important;\r\n        padding-bottom: 0 !important;\r\n        margin: 0 !important;\r\n        border-spacing: 0 !important;\r\n    }\r\n\r\n    td.v-align {\r\n        vertical-align: middle !important;\r\n    }\r\n\r\n    .bg {\r\n        background-color: #d2d2d2;\r\n    }\r\n\r\n    td.nopadding {\r\n        padding: 0 !important;\r\n    }\r\n\r\n    td.v-align-bt {\r\n        vertical-align: bottom !important;\r\n    }\r\n\r\n    .text-size {\r\n        font-size: 9px !important;\r\n        line-height: 1.1;\r\n    }\r\n\r\n    .bank {\r\n        line-height: 0.8 !important;\r\n    }\r\n</style>\r\n<div id=\"header-html\" class=\"hidden-pdf\">\r\n    {% if letter_head and not no_letterhead %}\r\n    <div class=\"letter-head\">{{ letter_head }}</div>\r\n    {% endif %}\r\n</div>\r\n<p class=\"text-size\" align=\"center\">\r\n    <strong>TAX INVOICE</strong>\r\n</p>\r\n<table border=\"1\" cellspacing=\"0\" cellpadding=\"0\" width=\"100%\" class=\"tbspace\">\r\n    <tbody>\r\n        <tr>\r\n            <td width=\"40%\" rowspan=\"3\" valign=\"top\">\r\n                <p class=\"text-size\"><strong>Exporter :</strong></p>\r\n                <p class=\"text-size\">\r\n                    <strong>\r\n                        {{ doc.company }}\r\n                    </strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    <strong>\r\n                        {{ frappe.db.get_value(\"Company\",doc.company,'company_description') or '' }}\r\n                    </strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'address_line1') or \"\" }}\r\n                </p>\r\n                {%- if frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'address_line2') -%}\r\n                <p class=\"text-size\">\r\n                    {{ frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'address_line2') or \"\" }}\r\n                </p>\r\n                {%- endif -%}\r\n                <p class=\"text-size\">\r\n                    {{ frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'city') or \"\" }}-\r\n                    {{ frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'pincode') or \"\" }},\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'state') or \"\" }}\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'country') or \"\" }}\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    IEC NO.{{ frappe.db.get_value(\"Company\", doc.company, 'iec_code') or \"\" }}\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    <strong>GST No. : {{ doc.company_gstin|upper or '' }}</strong>\r\n                </p>\r\n        \r\n            </td>\r\n            <td width=\"30%\" valign=\"top\" colspan = 2>\r\n                <p class=\"text-size\">\r\n                    <strong>Invoice No. &amp; Date :</strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.name|upper }}&nbsp;&nbsp;&nbsp; {{ doc.get_formatted(\"posting_date\") }}\r\n                </p>\r\n            </td>\r\n            <td width=\"30%\" valign=\"top\">\r\n                <p class=\"text-size\">\r\n                    <strong>Trade Agreement</strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.trade_agreement or '' }}\r\n                </p>\r\n            </td>\r\n        </tr>\r\n        <tr>\r\n            <td valign=\"top\" colspan = 2>\r\n                <!-- <p class=\"text-size\">\r\n                    <strong>Buyer's Order No. & Date :</strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.po_no|upper if doc.po_no else '' }}&nbsp;&nbsp;&nbsp;{{ doc.get_formatted(\"po_date\") or '' }}\r\n                </p> -->\r\n                <p class=\"text-size\">Vendor No&nbsp;:</p>\r\n            </td>\r\n            <td>\r\n                <p class=\"text-size\"><strong>Exporter Ref no and Date:</strong></p>\r\n                <p class=\"text-size\">{{ doc.exporter_ref_no or ''}}<br> {{ doc.get_formatted('exporter_ref_date') or ''}}</p>\r\n           \r\n            </td>\r\n        </tr>\r\n        <tr>\r\n            <td valign=\"top\" colspan = 2>\r\n                <p class=\"text-size\">\r\n                    <strong>Country of origin:</strong>\r\n                </p>\r\n                <p class=\"text-size\">{{ doc.country_of_origin|upper if doc.country_of_origin else '' }}</p>\r\n                <p class=\"text-size\">Buyer's Order No. & Date:{{doc.po_no}} {{doc.get_formatted('po_date')}}</p>\r\n            </td>\r\n            <td>\r\n                <p class=\"text-size\"><strong>Country of Destination :</strong></p>\r\n                <p class=\"text-size\">{{ doc.country_of_destination|upper if doc.country_of_destination else '' }}</p>\r\n            </td>\r\n        </tr>\r\n       \r\n        {% if doc.notify_address_display %}\r\n        <tr>\r\n            <td>\r\n                <p class=\"text-size\">\r\n                    <strong>Consignee&nbsp;:</strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                        <strong>\r\n                            {{ doc.consignee_name or doc.customer_name or '' }}\r\n                         </strong><br>\r\n                    {{ doc.shipping_address|upper if doc.shipping_address else '' }}\r\n                </p>\r\n            </td>\r\n            <td colspan = 2>\r\n                <p class=\"text-size\">\r\n                    <strong>Notify</strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    <p class=\"text-size\">{{frappe.db.get_value(\"Address\", doc.notify_party, \"address_title\")}}</p>\r\n                    <p class=\"text-size\">{{ doc.notify_address_display or ''}}</p>\r\n            </td>\r\n            <td>\r\n                <p class=\"text-size\">\r\n                    <strong>Notify Party Two</strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    <p class=\"text-size\">{{frappe.db.get_value(\"Address\", doc.notify_party_two, \"address_title\")}}</p>\r\n                    <p class=\"text-size\">{{ doc.notify_address_display_two or ''}}</p>\r\n            </td>\r\n        </tr>\r\n               \r\n        <tr>\r\n            <td width=\"40%\" rowspan=\"4\">\r\n                <p class=\"text-size\">\r\n                    <strong>Buyer</strong>(if other than Consignee)\r\n                </p>\r\n                 <p class=\"text-size\">\r\n                    <strong>{{ doc.customer_name }}</strong><br>\r\n                    {{ doc.address_display|upper if doc.address_display else '' }}\r\n                </p>\r\n            </td>\r\n            <td>\r\n                <p class=\"text-size\">\r\n                    <strong>\r\n                        Pre Carriage by\r\n                    </strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.pre_carriage_by or '' }} <br>\r\n                </p>\r\n            </td>\r\n            <td>\r\n                <p class=\"text-size\">\r\n                    <strong>\r\n                        Mode of Transport\r\n                    </strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.mode_of_transport or '' }} <br>\r\n                </p>\r\n            </td>\r\n            <td>\r\n                <p class=\"text-size\">\r\n                    <strong>\r\n                        Place of Precarrier\r\n                    </strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.place_of_precarrier or '' }}\r\n                </p>\r\n            </td>\r\n        </tr>\r\n        <tr>\r\n            <td colspan = 2>\r\n                <p class=\"text-size\"><strong>Port of Loading</strong></p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.port_of_loading|upper if doc.port_of_loading else '' }}\r\n                    {% if frappe.db.get_value(\"Port Details\",doc.port_of_loading,\"country\") %}\r\n                    ,{{ frappe.db.get_value(\"Port Details\",doc.port_of_loading,\"country\")|upper if frappe.db.get_value(\"Port Details\",doc.port_of_loading,\"country\") else '' }}\r\n                    {% endif %}\r\n                </p>\r\n            </td>\r\n            <td>\r\n                <p class=\"text-size\">\r\n                    <strong>Vessel/Flight No.</strong></p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.vessel_no|upper if doc.vessel_no else '' }}\r\n                    <br>\r\n                </p>\r\n            </td>\r\n        </tr>\r\n        <tr>\r\n            <td colspan = 2>\r\n                <p class=\"text-size\">\r\n                    <strong>\r\n                        Port of Discharge\r\n                    </strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.port_of_discharge|upper if doc.port_of_discharge else '' }}&nbsp;\r\n                    {% if frappe.db.get_value(\"Port Details\",doc.port_of_discharge,\"country\") %}\r\n                    ,{{ frappe.db.get_value(\"Port Details\",doc.port_of_discharge,\"country\")|upper if frappe.db.get_value(\"Port Details\",doc.port_of_discharge,\"country\") else '' }}\r\n                    {%endif%}\r\n                </p>\r\n            </td>\r\n            <td>\r\n                <p class=\"text-size\">\r\n                    <strong>\r\n                        Final Destination\r\n                    </strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.final_destination|upper if doc.final_destination else '' }}\r\n                </p>\r\n            </td>\r\n        </tr>\r\n        <tr>\r\n            <td colspan = 2>\r\n                <p class=\"text-size\">\r\n                    <strong>\r\n                        Shipping Bill Number. &amp; Date :\r\n                    </strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.shipping_bill_number or '' }}&nbsp;&nbsp;&nbsp; {{ doc.get_formatted(\"shipping_bill_date\") or '' }}\r\n                </p>\r\n                {% if doc.mode_of_transport == \"Air\"%}\r\n                {% if doc.awb_no %}\r\n                <p class=\"text-size\"><strong>AWB Number:</strong>{{doc.awb_no}}</p>\r\n                <p class=\"text-size\"><strong>AWB Date:</strong>{{doc.get_formatted('awb_date')}}</p>\r\n                {% endif %}\r\n                {% else %}\r\n                {% if doc.bl_no%}\r\n                <p class=\"text-size\"><strong>BL Number:</strong>{{doc.bl_no}}</p>\r\n                <p class=\"text-size\"><strong>BL Date:</strong>{{doc.get_formatted('bl_date')}}</p>\r\n                {% endif %}\r\n                {% endif %}\r\n\r\n            </td>\r\n            <td>\r\n                <p class=\"text-size\">\r\n                    <strong>Terms of Delivery and Payment</strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    PAYMENT : {{ doc.payment_terms_template|upper if doc.payment_terms_template else '' }}\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    DELIVERY : {{ doc.shipping_terms|upper if doc.shipping_terms else '' }}\r\n                </p>\r\n            </td>\r\n        </tr>\r\n        {% else %}\r\n        <tr>\r\n            <td rowspan=\"2\">\r\n                <p class=\"text-size\">\r\n                    <strong>Consignee&nbsp;:</strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                        <strong>\r\n                            {{ doc.consignee_name or doc.customer_name or '' }}\r\n                         </strong><br>\r\n                    {{ doc.shipping_address|upper if doc.shipping_address else '' }}\r\n                </p>\r\n            </td>\r\n            <td>\r\n                <p class=\"text-size\">\r\n                    <strong>\r\n                        Pre Carriage by\r\n                    </strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.pre_carriage_by or '' }} <br>\r\n                </p>\r\n            </td>\r\n            <td>\r\n                <p class=\"text-size\">\r\n                    <strong>\r\n                        Mode of Transport\r\n                    </strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.mode_of_transport or '' }} <br>\r\n                </p>\r\n            </td>\r\n            <td>\r\n                <p class=\"text-size\">\r\n                    <strong>\r\n                        Place of Precarrier\r\n                    </strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.place_of_precarrier or '' }}\r\n                </p>\r\n            </td>\r\n        </tr>\r\n        <tr>\r\n            <td colspan = 2>\r\n                <p class=\"text-size\"><strong>Port of Loading</strong></p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.port_of_loading|upper if doc.port_of_loading else '' }}\r\n                    {% if frappe.db.get_value(\"Port Details\",doc.port_of_loading,\"country\") %}\r\n                    ,{{ frappe.db.get_value(\"Port Details\",doc.port_of_loading,\"country\")|upper if frappe.db.get_value(\"Port Details\",doc.port_of_loading,\"country\") else '' }}\r\n                    {% endif %}\r\n                </p>\r\n            </td>\r\n            <td>\r\n                <p class=\"text-size\">\r\n                    <strong>Vessel/Flight No.</strong></p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.vessel_no|upper if doc.vessel_no else '' }}\r\n                    <br>\r\n                </p>\r\n            </td>\r\n        </tr>\r\n        <tr>\r\n            <td width=\"40%\" rowspan=\"2\">\r\n                <p class=\"text-size\">\r\n                    <strong>Buyer</strong>(if other than Consignee)\r\n                </p>\r\n                 <p class=\"text-size\">\r\n                    <strong>{{ doc.customer_name }}</strong><br>\r\n                    {{ doc.address_display|upper if doc.address_display else '' }}\r\n                </p>\r\n            </td>\r\n            <td colspan = 2>\r\n                <p class=\"text-size\">\r\n                    <strong>\r\n                        Port of Discharge\r\n                    </strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.port_of_discharge|upper if doc.port_of_discharge else '' }}&nbsp;\r\n                    {% if frappe.db.get_value(\"Port Details\",doc.port_of_discharge,\"country\") %}\r\n                    ,{{ frappe.db.get_value(\"Port Details\",doc.port_of_discharge,\"country\")|upper if frappe.db.get_value(\"Port Details\",doc.port_of_discharge,\"country\") else '' }}\r\n                    {% endif %}\r\n                </p>\r\n            </td>\r\n            <td>\r\n                <p class=\"text-size\">\r\n                    <strong>\r\n                        Final Destination\r\n                    </strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.final_destination|upper if doc.final_destination else '' }}\r\n                </p>\r\n            </td>\r\n        </tr>\r\n       \r\n        <tr>\r\n            <td colspan = 2>\r\n                <p class=\"text-size\">\r\n                    <strong>\r\n                        Shipping Bill Number. &amp; Date :\r\n                    </strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    {{ doc.shipping_bill_number or '' }}&nbsp;&nbsp;&nbsp; {{ doc.get_formatted(\"shipping_bill_date\") or '' }}\r\n                </p>\r\n                {% if doc.mode_of_transport == \"Air\"%}\r\n                {% if doc.awb_no %}\r\n                <p class=\"text-size\"><strong>AWB Number:</strong>{{doc.awb_no}}</p>\r\n                <p class=\"text-size\"><strong>AWB Date:</strong>{{doc.get_formatted('awb_date')}}</p>\r\n                {% endif %}\r\n                {% else %}\r\n                {% if doc.bl_no%}\r\n                <p class=\"text-size\"><strong>BL Number:</strong>{{doc.bl_no}}</p>\r\n                <p class=\"text-size\"><strong>BL Date:</strong>{{doc.get_formatted('bl_date')}}</p>\r\n                {% endif %}\r\n                {% endif %}\r\n            </td>\r\n            <td>\r\n                <p class=\"text-size\">\r\n                    <strong>Terms of Delivery and Payment</strong>\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    PAYMENT : {{ doc.payment_terms_template|upper if doc.payment_terms_template else '' }}\r\n                </p>\r\n                <p class=\"text-size\">\r\n                    DELIVERY : {{ doc.shipping_terms|upper if doc.shipping_terms else '' }} -\r\n                    {{ doc.shipping_destination or ''}}\r\n                </p>\r\n            </td>\r\n        </tr>\r\n        {% endif %}\r\n        <tr>\r\n            <td width=\"100%\" colspan=\"4\" class=\"nopadding\" style=\"padding: 0!important;\">\r\n                <table border=\"1\" cellspacing=\"0\" cellpadding=\"0\" width=\"100%\" class=\"tbspace-sub\">\r\n                    <tbody>\r\n                        <tr>\r\n                            <td width=\"16%\" class=\"v-align\">\r\n                                <p class=\"text-size\" align=\"center\">\r\n                                    <strong>MARKS & No/<br>Container No.</strong>\r\n                                </p>\r\n                            </td>\r\n                            <td width=\"10%\" class=\"v-align\">\r\n                                <p class=\"text-size\" align=\"center\">\r\n                                    <strong>No. & Kind of<br>Packages</strong>\r\n                                </p>\r\n                            </td>\r\n                            <td width=\"22%\" class=\"v-align\">\r\n                                <p class=\"text-size\" align=\"center\">\r\n                                    <strong>Description of Goods</strong>\r\n                                </p>\r\n                                <p class=\"text-size\" align=\"center\">\r\n                                    <strong>{% if doc.description_of_goods %}{{ doc.description_of_goods.replace('\\n','<br>')}}{% endif %}</strong>\r\n                                </p>\r\n                            </td>\r\n                            <td width=\"8%\" class=\"v-align\">\r\n                                <p class=\"text-size\" align=\"center\">\r\n                                    <strong>Qty {{ doc.items[0].uom }}\r\n                                    </strong>\r\n                                </p>\r\n                            </td>\r\n                            <td width=\"7%\" class=\"v-align\">\r\n                                <p class=\"text-size\" align=\"center\">\r\n                                    <strong>Rate<br>{{ doc.currency }}/ {{ doc.items[0].uom }}</strong>\r\n                                </p>\r\n                            </td>\r\n                            <td width=\"9%\" class=\"v-align\" align=\"center\">\r\n                                <p class=\"text-size\">\r\n                                    <strong>Amount</strong>\r\n                                    <strong>{{ doc.currency }}</strong>\r\n                                </p>\r\n                            </td>\r\n                            <td width=\"11%\" class=\"v-align\" align=\"center\">\r\n                                <p class=\"text-size\">\r\n                                    <strong>Amount</strong>\r\n                                    <strong>{{ doc.price_list_currency }}</strong>\r\n\r\n                                </p>\r\n                                <p class=\"text-size\"><strong>({{ doc.conversion_rate or '' }})</strong></p>\r\n                            </td>\r\n                            <td width=\"10%\" class=\"v-align\" align=\"center\">\r\n                                <p class=\"text-size\">\r\n                                    <strong>IGST Amount</strong>\r\n                                </p>\r\n                            </td>\r\n                        </tr>\r\n\r\n                        {%- for row in doc.items -%}\r\n                        <tr>\r\n                            {%- if loop.index == 1-%}\r\n                            <td valign=\"top\" align=\"left\" class=\"notopbottom\" rowspan=\"{{loop.length}}\">\r\n                                <p class=\"text-size\">\r\n                                    {% if doc.marks_and_no %}{{ doc.marks_and_no.replace('\\n','<br>') or '' }}{% endif %}\r\n                                </p>\r\n                            </td>\r\n                            {% endif %}\r\n                            <td valign=\"top\" align=\"center\" class=\"notopbottom\">\r\n                                <p class=\"text-size\">\r\n                                    {%- if row.packaging_material -%}\r\n                                    {{ row.no_of_packages or '' }} {{ row.packaging_material or ''}} &#10005;\r\n                                    {{ row.packing_size or ''}}{{ row.uom }}\r\n\r\n                                    {% endif %}\r\n                                    {% if not row.dont_show_package_no %}\r\n                                    <br> {{ row.packaging_material or ''}} : {{ row.packages_from }} to\r\n                                    {{ row.packages_to }}\r\n                                    {% endif %}\r\n\r\n                                </p>\r\n                            </td>\r\n                            <td valign=\"top\" class=\"notopbottom\">\r\n                                <p style=\"font-size: 10px!important\">\r\n                                    {{ row.item_name|upper }}\r\n                                </p>\r\n                                <p style=\"font-size: 10px!important\">\r\n                                    (HS Code:&nbsp;{{ row.gst_hsn_code or '' }})\r\n                                </p>\r\n                            </td>\r\n\r\n                            <td valign=\"top\" align=\"center\" class=\"notopbottom\">\r\n                                <p class=\"text-size\">\r\n                                    {{ row.qty or '' }}\r\n                                </p>\r\n                            </td>\r\n                            <td valign=\"top\" align=\"right\" class=\"notopbottom\">\r\n                                <p class=\"text-size\">\r\n                                    {{ row.get_formatted(\"rate\") or '' }}\r\n                                </p>\r\n                            </td>\r\n                            <td class=\"notopbottom\" align=\"right\">\r\n                                <p class=\"text-size\">\r\n                                    {{ row.get_formatted(\"amount\") or '' }}\r\n                                </p>\r\n                            </td>\r\n                            <td class=\"notopbottom\" align=\"right\">\r\n                                <p class=\"text-size\">\r\n                                    {{ row.get_formatted(\"base_amount\") or '' }}\r\n                                </p>\r\n                            </td>\r\n                            <td class=\"notopbottom\" align=\"right\">\r\n                                <p class=\"text-size\">\r\n                                    {{ row.get_formatted(\"igst_amount\") or '' }}\r\n                                </p>\r\n                            </td>\r\n                        </tr>\r\n       